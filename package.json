{
  "name": "obsidian-vertex-ai-mastermind",
  "version": "0.0.1",
  "description": "A mastermind AI assistant for Obsidian leveraging Google Vertex AI.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "version": "node -e \"let m=JSON.parse(fs.readFileSync('manifest.json','utf-8')); m.version=process.env.npm_package_version; fs.writeFileSync('manifest.json', JSON.stringify(m, null, '\\t'));\""
  },
  "keywords": [
    "obsidian",
    "vertex-ai",
    "ai",
    "assistant",
    "mastermind"
  ],
  "author": "Antigravity",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.21",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^6.1.0",
    "@google-cloud/vertexai": "^1.1.0"
  }
}
